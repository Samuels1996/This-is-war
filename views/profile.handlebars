<div>
  <div>
    <h2>Welcome, {{username}}!</h2>
  </div>
</div>

<div>
  <div>
    <h3>Recruit a Champion:</h3>

    <form class="form new-champ-form">
      <div class="form-group">
        
    </form>
  </div>

  {{#if champions.length}}
    <div class="champ-list">
      <h3>Champions:</h3>

      {{#each champions as |champion|}}
        <div>
          <div>
            <h4><a href="/champion/{{champion.id}}">{{champion.title}}</a></h4>
          </div>
          <div>
            <button
              class="btn btn-sm btn-danger"
              data-id="{{champion.id}}"
            >BANISH</button>
          </div>
        </div>
      {{/each}}
    </div>
  {{/if}}
</div>

<script src="./js/profile.js"></script>