<div class="container-sm">
    <div class="card align-left col=sm">
  <div class="card-header fantasy">
    Your Champion
  </div>
  <div class="card-body">
    <blockquote class="blockquote mb-0">
      <p id="champHp">Hitpoints:{{champion.hitpoints}}</p>
        <p id="champStr">Strength:{{champion.strength}}</p>
    </blockquote>
  </div>
</div>
<div class="card align-right col-sm">
  <div class="card-header fantasy">
    Enemy Champion
  </div>
  <div class="card-body">
    <blockquote class="blockquote mb-0">
      <p id="enemyHp">Hitpoints:{{enemy.hitpoints}}</p>
        <p id="enemyStr">Strength:{{enemy.strength}}</p>
    </blockquote>
  </div>
</div>

<button class="button" type="button" id="combat">Fight!</button>
</div>
<script src="./js/fight.js"></script>